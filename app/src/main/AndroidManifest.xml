<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ygz.dm">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <!--震动-->
    <uses-permission android:name="android.permission.VIBRATE" />

    <application
        android:name=".config.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">


        <activity
            android:name=".MainActivity"
            android:launchMode="singleTask" />
        <activity android:name=".StartActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LoginActivity" />
        <activity android:name=".Register" />
        <activity android:name=".TestActivity" />
        <activity android:name=".ForgetPasswordActivity" />
        <activity android:name=".ForgetRePasswordActivity" />
        <activity android:name=".Register2Activity" />
        <activity android:name=".AddEventActivity" />
        <activity android:name=".EventDetailsActivity" />
        <activity android:name=".EventUpdateActivity" />
        <activity android:name=".TrashActivity" />
        <activity android:name=".MyInfoActivity" />
        <activity android:name=".AboutActivity"></activity>
    </application>

</manifest>